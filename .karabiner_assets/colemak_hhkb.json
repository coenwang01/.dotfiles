{
	"title": "Own Custom Keymaps",
	"rules": [
		{
			"description": "Tab to Cmd on press and hold.",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "tab",
						"modifiers": {
							"optional": ["any"]
						}
					},
					"to": [
						{
							"key_code": "left_command"
						}
					],
					"to_if_alone": [
						{
							"key_code": "tab"
						}
					]
				}
			]
		},
		{
			"description": "Space to Hyper on press and hold",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "spacebar",
						"modifiers": {
							"optional": ["any"]
						}
					},
					"to": [
						{
							"key_code": "left_shift",
							"modifiers": ["left_command", "left_control", "left_option"]
						}
					],
					"to_if_alone": [
						{
							"key_code": "spacebar"
						}
					]
				}
			]
		},
