{
	"title": "Own Custom Keymaps",
	"rules": [
		{
			"description": "Tab to Cmd on press and hold.",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "tab",
						"modifiers": {
							"optional": ["any"]
						}
					},
					"to": [
						{
							"key_code": "left_command"
						}
					],
					"to_if_alone": [
						{
							"key_code": "tab"
						}
					]
				}
			]
		},
		{
			"description": "Right Commant to Hyper",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "right_command",
						"modifiers": {
							"optional": ["any"]
						}
					},
					"to": [
						{
							"key_code": "left_shift",
							"modifiers": ["left_command", "left_control", "left_option"]
						}
					]
				}
			]
		},
		{
			"description": "Left Command to Fn",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "left_command",
						"modifiers": {
							"optional": ["any"]
						}
					},
					"to": [
						{
							"key_code": "fn"
						}
					]
				}
			]
		}
	]
}
