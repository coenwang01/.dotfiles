{
	"title": "Own Custom Keymaps",
	"rules": [
		{
			"description": "Tab to Cmd on press and hold.",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "tab",
						"modifiers": {
							"optional": ["any"]
						}
					},
					"to": [
						{
							"key_code": "left_command"
						}
					],
					"to_if_alone": [
						{
							"key_code": "tab"
						}
					]
				}
			]
		},
		{
			"description": "Left Control to Escape On single press, Control on press and hold",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "left_control",
						"modifiers": {
							"optional": ["any"]
						}
					},
					"to": [
						{
							"key_code": "left_control"
						}
					],
					"to_if_alone": [
						{
							"key_code": "escape"
						}
					]
				}
			]
		},
