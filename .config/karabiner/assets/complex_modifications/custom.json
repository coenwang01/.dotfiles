				{
					"conditions": [
						{
							"name": "yabai_mode",
							"type": "variable_if",
							"value": 1
						}
					],
					"from": {
						"key_code": "l",
						"modifiers": {
							"optional": ["any"]
						}
					},
					"to": [
						{
							"shell_command": "opt/homebrew/bin/yabai -m window --focus east"
						}
					],
					"type": "basic"
				},
				{
					"conditions": [
						{
							"name": "yabai_mode",
							"type": "variable_if",
							"value": 1
						}
					],
					"from": {
						"key_code": "j",
						"modifiers": {
							"mandatory": ["shift"]
						}
					},
					"to": [
						{
							"shell_command": "opt/homebrew/bin/yabai -m window --resize left:-50:0; \\ yabai -m window --resize right:-50:0"
						}
					],
