		{
			"description": "Normal mode",
			"manipulators": [
				{
					"from": {
						"key_code": "escape",
						"modifiers": {
							"mandatory": ["command", "option", "control", "shift"]
						}
					},
					"to": [
						{
							"set_variable": {
								"name": "yabai_mode",
								"value": 0
							}
						}
					],
					"type": "basic"
				}
			]
		},
		{
			"description": "yabai mode",
			"manipulators": [
				{
					"from": {
						"key_code": "spacebar",
						"modifiers": {
							"mandatory": ["command", "option", "control", "shift"]
						}
					},
